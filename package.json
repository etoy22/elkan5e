{
	"name": "elkan5e",
	"version": "1.0.0",
	"description": "![GitHub Downloads](https://img.shields.io/github/downloads/etoy22/elkan5e/module.zip?color=blue)",
	"main": "elkan5e.mjs",
	"type": "module",
	"scripts": {
		"update": "npm run prep:data && npm run clean",
		"prep:data": "npm run update:spelllists && npm run update:features && npm run update:summoned",
		"update:features": "node ./src/tools/sync-features.mjs",
		"update:spelllists": "node ./src/tools/update-spell-lists.mjs",
		"update:summoned": "node ./src/tools/sync-summoned.mjs",
		"pack": "npm run fix && npm run build:clean && npm run build:db",
		"pack:single": "node ./src/packs.mjs package pack",
		"clean": "npm run fix && npm run build:clean",
		"unpack": "node ./src/packs.mjs package unpack && npm run prep:data",
		"build:clean": "node ./src/packs.mjs package clean",
		"build:db": "node ./src/packs.mjs package pack",
		"remove": "node ./src/packs.mjs package remove",
		"lint": "eslint \"src/**/*.mjs\"",
		"lint:fix": "eslint --fix \"src/**/*.mjs\"",
		"format": "prettier --config .prettierrc.json --write \"src/**/*.{js,mjs,json,md}\"",
		"test": "node --test src/tools/tests.mjs",
		"fix": "npm run lint:fix && npm run format"
	},
	"devDependencies": {
		"@foundryvtt/foundryvtt-cli": "^1.0.3",
		"eslint": "^9.32.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.4",
		"node-fetch": "^2.7.0",
		"prettier": "^3.6.2",
		"yaml": "^2.8.1"
	},
	"dependencies": {
		"@foundryvtt/foundryvtt-cli": "^1.0.3",
		"fancy-log": "^2.0.0",
		"yargs": "^17.7.2"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/etoy22/elkan5e.git"
	},
	"author": "Ethan Leider",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/etoy22/elkan5e/issues"
	},
	"homepage": "https://github.com/etoy22/elkan5e#readme"
}
