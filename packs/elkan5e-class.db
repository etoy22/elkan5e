":"Features","icon":"","classRestriction":""},{"_id":"yewie41jrdc452qp","type":"ItemGrant","configuration":{"items":["Compendium.elkan5e.elkan5e-class-features.JFPoSGMda48Rp4Ys"]},"value":{},"level":20,"title":"Features","icon":"","classRestriction":""},{"_id":"81ogni84p49yv859","type":"HitPoints","configuration":{},"value":{},"title":"","icon":"","classRestriction":""},{"_id":"zmu0w24zpmg5hahq","type":"ScaleValue","configuration":{"identifier":"die","type":"dice","scale":{"1":{"n":1,"die":6},"5":{"n":1,"die":8},"11":{"n":1,"die":10},"17":{"n":1,"die":12}}},"value":{},"title":"Martial Arts Die","icon":"","classRestriction":""},{"_id":"ng597k2bjuwpfys8","type":"ScaleValue","configuration":{"identifier":"unarmored-movement","type":"distance","distance":{"units":"ft"},"scale":{"2":{"value":10},"6":{"value":15},"10":{"value":20},"14":{"value":25},"18":{"value":30}}},"value":{},"title":"Unarmored Movement","icon":"","classRestriction":""},{"_id":"tv7jwhokk6simr4f","type":"ItemGrant","configuration":{"items":["Compendium.elkan5e.elkan5e-class-features.AnzFd1P7OeHhj1EA"],"optional":false},"value":{},"level":9,"title":"Features","icon":"","classRestriction":""},{"_id":"jqfvc0sif34998w1","type":"ItemGrant","configuration":{"items":["Compendium.elkan5e.elkan5e-class-features.y1yr9h2t8wsCBcsw"],"optional":false},"value":{},"level":1,"title":"Default","icon":"","classRestriction":"primary"},{"_id":"uex6bgd3w5pcg2op","type":"ItemGrant","configuration":{"items":["Compendium.elkan5e.elkan5e-class-features.ivGqiWd5wfvECJBY"],"optional":false},"value":{},"level":1,"title":"Default","icon":"","classRestriction":"secondary"}],"saves":["str","dex"],"skills":{"number":2,"choices":["acr","ath","his","ins","rel","ste"],"value":[]},"spellcasting":{"progression":"none","ability":""},"damage":{"parts":[]}},"ownership":{"default":0,"ymJDAw0PgXIWIHMH":3},"_stats":{"systemId":"dnd5e","systemVersion":"2.0.2","coreVersion":"10.285","createdTime":null,"modifiedTime":1663537685103,"lastModifiedBy":