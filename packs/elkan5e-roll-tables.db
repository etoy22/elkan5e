:null,"flags":{}},{"type":0,"weight":1,"range":[17,17],"drawn":true,"documentCollection":"","_id":"9YAqAgcUKnAiX57q","text":"","img":"icons/svg/d20-black.svg","documentId":null,"flags":{}},{"type":0,"weight":1,"range":[18,18],"drawn":false,"documentCollection":"","_id":"irgmGtnOORXeYSz0","text":"You regain 8 sorcery points, up to your normal maximum.","img":"icons/svg/d20-black.svg","documentId":null,"flags":{}},{"type":0,"weight":1,"range":[19,19],"drawn":false,"documentCollection":"","_id":"iXShYS30i95kkkFG","text":"You can choose to have your spell be affected by any one additional metamagic ability you know without costing sorcery points.","img":"icons/svg/d20-black.svg","documentId":null,"flags":{}},{"type":2,"weight":1,"range":[20,20],"drawn":false,"text":"Wild Surge Table Lv. 9","img":"icons/svg/d20-grey.svg","documentCollection":"elkan5e.elkan5e-roll-tables","documentId":"LV2skOm8hCwM1JRH","_id":"2nT6zyguTBTfkhdX","flags":{}}],"formula":"1d20","replacement":true,"displayRoll":true,"ownership":{"default":0,"MKYWAnkTJr0gkDqT":3},"flags":{"core":{"sourceId":"RollTable.JBXC52WBXccp1kKi"}},"_stats":{"systemId":"dnd5e","systemVersion":"2.1.5","coreVersion":"10.291","createdTime":1680044396193,"modifiedTime":1680044618407,"lastModifiedBy":"MKYWAnkTJr0gkDqT"},"folder":null,"sort":0,"_id":"xkUpS2XBuSdyVEah"}
