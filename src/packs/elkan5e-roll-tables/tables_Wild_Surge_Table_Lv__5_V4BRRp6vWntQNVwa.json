            "range": [
                5,
                5
            ],
            "drawn": false,
            "documentCollection": "",
            "_id": "Z3ApLUzkT7KPUGqd",
            "text": "A small raincloud appears above the head of someone you choose within 120 ft. It rains on them for 1 minute. If you can't see anyone, it rains on you.",
            "img": "icons/svg/d20-black.svg",
            "documentId": null,
            "flags": {},
            "_key": "!tables.results!V4BRRp6vWntQNVwa.Z3ApLUzkT7KPUGqd"
