            "drawn": false,
            "documentCollection": "",
            "_id": "vfnHWLjlvOlfzX8B",
            "text": "A creature of your choice within 90 ft. must make a wisdom save or be goaded by an article of your clothing of your choice for 1 round.",
            "img": "icons/svg/d20-black.svg",
            "documentId": null,
            "flags": {},
