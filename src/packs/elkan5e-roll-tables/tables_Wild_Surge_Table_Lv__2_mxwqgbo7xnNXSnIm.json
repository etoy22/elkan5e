      "documentCollection": "",
      "_id": "csdMvQLzq4KmSW5e",
      "text": "As soon as your spell is finished casting, magical darkness appears in a 20 ft. radius from you.",
      "img": "icons/svg/d20-black.svg",
      "documentId": null,
      "flags": {},
      "_key": "!tables.results!mxwqgbo7xnNXSnIm.csdMvQLzq4KmSW5e"
