        2
      ],
      "drawn": false,
      "documentCollection": "",
      "_id": "TFSMV5A9dRRo2XEp",
      "text": "You become drunk on your own power, and are poisoned for 1 minute.",
      "img": "icons/svg/d20-black.svg",
      "documentId": null,
      "flags": {},
      "_key": "!tables.results!0TDp89O9iGt4zovG.TFSMV5A9dRRo2XEp"
    },
