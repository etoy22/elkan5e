      ],
      "drawn": false,
      "documentCollection": "",
      "_id": "aa7Upmji0SnSLjpZ",
      "text": "You are forced to cast a different spell using the same spell slot, losing your original spell.",
      "img": "icons/svg/d20-black.svg",
      "documentId": null,
      "flags": {},
      "_key": "!tables.results!0TDp89O9iGt4zovG.aa7Upmji0SnSLjpZ"
